                                    <_screenShotBase64 i:nil="true" />
                                    <_value xmlns:d19p1="http://www.w3.org/2001/XMLSchema" i:type="d19p1:string">/*

Code:

Codigo realizado para filtrar inicialmente los pedidos pendientes validos. En base a:

- Fecha de pedido; indica la fecha del pedido y permitira evitar que el Bot recorra filas demas.
- Estado Produccion; indica si el pedido ya se encuentra disponible para el envio.
- Estado Despacho; indica si el pedido ha sido o no despachado.
- Item; es un ID que identifica el pedido con un elemento de la empresa.

Las condicionales de un pedido pendiente es:

1. Fecha de pedido despues de la ultima fecha de revision del sheets por parte del Bot
2. Estado de despacho igual a "Pendiente"
3. Estado de Produccion debe ser "Inventario" o "Empacado"
4. Item debe ser diferente a vacio.

*/

bool ban_fecha; //Indica que la fecha sea valida
string[] fecha = ultima_Fecha.Split("/"); //Separador de dia, mes y año de la ultima fecha de revision
DataTable aux_table = SheetsTable.Clone(); //Variable auxiliar

for (int i = SheetsTable.Rows.Count - 1; i &gt;= 0; i--)
{
    DataRow fila = SheetsTable.Rows[i];
    ban_fecha = false;
    
    //Filtro inicial: fecha de pedido
    if(fila["Fecha de Pedido"].ToString().Contains(fecha[2])) //Comprobacion de año correcto
    {
        string[] fecha_sheets = fila["Fecha de Pedido"].ToString().Split("/");
        
        if(int.Parse(fecha_sheets[1]) &gt; int.Parse(fecha[1])) //Comprobacion, mes siguiente
        {
            ban_fecha = true;
        }
        else if(int.Parse(fecha_sheets[1]) == int.Parse(fecha[1])) //Comprobacion: mismo mes
        {
            if(int.Parse(fecha_sheets[0]) &gt; int.Parse(fecha[0])) //Dia siguiente
            {
                ban_fecha = true;
            }
        }
    }
    else
    {
        break;
    }
    
    //Filtro Principal: Estado de despacho, Estado de produccion e Item
    if(ban_fecha == true)
    {
    // &amp;&amp; (!fila["Item"].Equals(""))
        /*if((fila["Estado Despacho"].Equals("Pendiente")) &amp;&amp; ((fila["Estado Producción"].Equals("Inventario")) || (fila["Estado Producción"].Equals("Empacado"))))
        {
            aux_table.ImportRow(fila);
            list_AuxRowsSheets.Add(i+3);
        }*/
        aux_table.ImportRow(fila);
        list_AuxRowsSheets.Add(i+3);
    }
}

SheetsTable = aux_table;</_value>
